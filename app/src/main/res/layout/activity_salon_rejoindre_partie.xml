<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SalonCreerPartie"
    android:background="@drawable/fond">

    <ImageButton
        android:id="@+id/pret"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_pret_btn"/>

    <ImageButton
        android:id="@+id/backToMenuSalonRejoindrePartie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_retour_btn"/>

    <LinearLayout
        android:id="@+id/cadreJoueursRejoindrePartie"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:background="@drawable/ic_parchemin"/>



</RelativeLayout>